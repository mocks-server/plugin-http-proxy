  "devDependencies": {
    "@mocks-server/core": "2.4.0",
    "cross-fetch": "3.1.4",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-no-only-tests": "2.6.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "is-ci": "3.0.0",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "sinon": "10.0.0",
    "wait-on": "5.3.0"
