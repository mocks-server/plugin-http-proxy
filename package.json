{
  "name": "@mocks-server/plugin-proxy",
  "version": "2.1.0",
  "description": "Mocks Server plugin providing an http-proxy route handler",
  "keywords": [
    "mocks-server-plugin",
    "proxy",
    "express-http-proxy",
    "reverse",
    "http",
    "https",
    "mock",
    "testing",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/plugin-proxy",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
